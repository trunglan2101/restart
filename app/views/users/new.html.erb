<div class="container">
  <%= form_for @users do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :email %>
    <%= f.email_field :email %>

    <%= f.label :password %>
    <% f.password_field :password %>

    <%= f.label :password_confirmation %>
    <%= f.password_confirmation_field :password %>

    <%= f.submit %>
  <% end %>
</div>
